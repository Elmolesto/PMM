<div class="page-header">
  <%= link_to mmpps_path, class: 'btn btn-secondary' do %>
    MMPP
  <% end %>
  <%= link_to edit_mmpp_path(@mmpp), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Ver MMPP</h1>
</div>

<dl class="dl-horizontal">
  <dt>Nombre:</dt>
